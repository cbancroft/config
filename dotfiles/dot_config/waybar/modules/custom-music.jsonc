{
  "custom/music": {
    "format": "{0}",
    "return-type": "json",
    "max-length": 100,
    "escape": true,
    "tooltip": true,
    "exec": "~/.config/waybar/scripts/mediaplayer.py --player spotify",
    "on-click": "playerctl play-pause",
    "on-scroll-up": "playerctl next",
    "on-scroll-down": "playerctl previous",
  },
}
